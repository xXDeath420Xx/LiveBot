    "axios": "^1.7.2",
    "bullmq": "^5.7.9",
    "canvas": "^2.11.2",
    "connect-redis": "^6.1.3",
    "cycletls": "^1.0.22",
    "discord-player": "^7.1.0",
    "discord.js": "^14.15.3",
    "dotenv": "^16.4.5",
    "dotenv-flow": "^4.1.0",
    "ejs": "^3.1.10",
    "elevenlabs": "^1.59.0",
    "express": "^4.19.2",
    "express-rate-limit": "^8.1.0",
    "express-session": "^1.18.0",
    "fast-xml-parser": "^4.2.5",
