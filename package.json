{
  "name": "certifried-announcer-stable",
  "version": "0.3.0",
  "description": "A scalable, professional, multi-platform stream announcer bot.",
  "main": "src/init/index.js",
  "scripts": {
    "start": "npm run lint && npm run build && node dist/sharding.js",
    "deploy": "node src/init/deploy-commands.js",
    "dev": "npm run clean && NODE_ENV=development npm run start",
    "major": "npm version major",
    "minor": "npm version minor",
    "patch": "npm version patch",
    "build": "node_modules/.bin/tsc --build --verbose",
    "clean": "node_modules/.bin/del-cli --force dist",
    "clean-start": "npm run clean && npm run start",
    "format": "node_modules/.bin/prettier --write src",
    "lint": "node_modules/.bin/eslint ./src --ext .ts,.tsx"
  },
  "dependencies": {
    "axios": "^1.7.2",
    "bullmq": "^5.7.13",
    "cycletls": "^1.0.22",
    "discord.js": "^14.15.3",
    "dotenv-flow": "^4.1.0",
    "ejs": "^3.1.10",
    "express": "^4.19.2",
    "express-session": "^1.18.0",
    "ioredis": "^5.4.1",
    "multer": "2.0.2",
    "mysql2": "^3.10.1",
    "papaparse": "^5.4.1",
    "passport": "^0.7.0",
    "passport-discord": "^0.1.4",
    "playwright": "^1.45.1",
    "pm2": "^6.0.13",
    "tough-cookie": "^4.1.3",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "del-cli": "^5.0.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^8.10.0",
    "prettier": "^2.8.8",
    "simple-eta": "^3.0.2",
    "tsx": "^4.7.3",
    "typescript": "^4.9.5"
  },
  "prettier": {
    "trailingComma": "es5",
    "semi": true,
    "bracketSpacing": true,
    "arrowParens": "always"
  }
}
