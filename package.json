{
  "name": "certifried-multitool",
  "version": "4.0.0",
  "description": "A comprehensive Discord multi-tool bot featuring stream announcements, moderation, music with AI DJ, leveling, giveaways, polls, tickets, forms, feeds, and 20+ other features.",
  "main": "index.js",
  "scripts": {
    "start": "pm2 start ecosystem.config.js --env production",
    "dev": "pm2 start ecosystem.config.js --env development",
    "stop": "pm2 stop all",
    "delete": "pm2 delete all",
    "logs": "pm2 logs",
    "deploy": "node deploy-commands.js",
    "schedule:teams": "node jobs/team-sync-scheduler.js",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
